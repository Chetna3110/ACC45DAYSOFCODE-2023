#include <stdio.h>
int main(void) {
	int t;
	scanf("%d",&t);
	while(t--){
	    int A,B,C,X;
	    scanf("%d",&A);
	    scanf("%d",&B);
	    scanf("%d",&C);
	    scanf("%d",&X);
	    if((A+B)>=X||(B+C)>=X||(C+A)>=X){
	        printf("yes\n");
	    }
	    else{
	    printf("No\n");
	    }
	}
	return 0;
